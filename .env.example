PORT=5000
NODE_ENV=development

# ScrapeCreators
SCRAPECREATORS_API_KEY=YOUR_KEY

# AWS
AWS_REGION=ap-south-1
AWS_ACCESS_KEY_ID=YOUR_ACCESS_KEY
AWS_SECRET_ACCESS_KEY=YOUR_SECRET
AWS_S3_BUCKET=passionbits-core-files

# Optional CDN base (CloudFront/custom domain)
AWS_CDN_BASE_URL=https://assets.passionbits.io

# FFmpeg path:
# Windows example: C:\ffmpeg\bin\ffmpeg.exe
# Linux/Mac example: /usr/bin/ffmpeg
FFMPEG_PATH=/usr/bin/ffmpeg
